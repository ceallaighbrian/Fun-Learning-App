<% provide(:title, @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user-info">
      <h1> <%= @user.name %> </h1>
      <% Quiz.all.each do |q| %>

          <h3><%= q.name %> </h3>

          <h3>Quiz Attempts <%= @entries.where("quiz_id = #{q.id}").count %></h3>

      <% end %>


    </section>
  </aside>
</div>